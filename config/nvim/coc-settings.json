{
  "coc.preferences.enableFloatHighlight": true,
  "suggest.enablePreview": true,
  "suggest.labelMaxLength": 30,
  "eslint.autoFixOnSave": true,
  "list.maxPreviewHeight": 60,
  "prettier.disableSuccessMessage": true,
  "snippets.snipmate.enable": false,
  "snippets.userSnippetsDirectory": "~/.vim/snippets",
  "snippets.editSnippetsCommand": ":vsplit",
  "solargraph.checkGemVersion": false,
  "solargraph.autoformat": false,
  "solargraph.completion": true,
  "solargraph.diagnostics": true,
  "solargraph.definitions": true,
  "solargraph.folding": true,
  "solargraph.formatting": true,
  "solargraph.hover": true,
  "solargraph.rename": true,
  "solargraph.references": true,
  "solargraph.symbols": true,
  "stylelintplus.autoFixOnSave": true,
  "stylelintplus.autoFixOnFormat": true,
  "stylelintplus.validateOnType": true,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "wxss.validate": false,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit"
  },
  "git.enableGutters": true,
  "vetur.format.enable": true,
  "json.format.enable": true,
  "yaml.format.enable": true,
  "yaml.schemas": {
    "https://raw.githubusercontent.com/DavidAnson/markdownlint/main/schema/markdownlint-config-schema.json": "/.markdownlint.yaml"
  },
  "yaml.completion": true,
  "html.format.enable": true,
  "typescript.format.enabled": true,
  "javascript.format.enabled": true,
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform", "tf", "hcl"],
      "initializationOptions": {},
      "settings": {}
    }
  }
}
